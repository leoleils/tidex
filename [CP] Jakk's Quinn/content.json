{
  "Format": "2.1.0",
  "ConfigSchema": {
    "Seasonal cuter Quinn": {
      "AllowValues": "True, False",
      "Default": "False",
      "Description": "Seasonal Outfits made to go match Poltergeister's 'Seasonal Outfits - Slightly Cuter Aesthetic'.",
      "Section": "Poltergeister's Seasonal Cute Characters Compatibility",
      "AllowBlank": false
    },
    "Has nose": {
      "AllowValues": "True, False",
      "Default": "True",
      "Description": "Nose overlay. 'True' will look closer to vanilla, 'False' will fit alongside Poltergeister's 'Seasonal Outfits - Slightly Cuter Aesthetic'.",
      "Section": "Poltergeister's Seasonal Cute Characters Compatibility",
      "AllowBlank": false
    },
    "New mail paper": {
      "AllowValues": "True, False",
      "Default": "True",
      "Description": "Allows Quinn's mail to be sent in custom mail paper. Set to false if there are bugs related to mail form other mods.",
      "Section": "Generic Fixes",
      "AllowBlank": false
    },
    "Include Quinn in CT": {
      "AllowValues": "True, False",
      "Default": "True",
      "Description": "Includes Quinn in Emily's 8 Hearts event. This config exists for better compatibility with mods that may change Emily's cutscenes. Extremely miniscule narrative impact.",
      "Section": "Generic Fixes",
      "AllowBlank": false
    },
    "Meet up day": {
      "AllowValues": "4, 5, 6, 7, 9, 10, 14, 18, 19, 26",
      "Default": "7",
      "Description": "If you have SVE installed, Quinn and some NPCs might meet up once a month to chat. Standard is every 7th of every month, but feel free to customize it if so needed. Some days are purposefully ommited as to avoid conflicts with characters' schedules and festivals. Note that characters that work will not work if you pick a weekday.",
      "Section": "SVE Fixes",
      "AllowBlank": false
    },
    "Outfit": {
      "AllowValues": "Cosplay, Fair, Fall, Jellies, Joja, LakeSwim, Luau, Spring, Summer, Winter",
      "Default": "Spring",
      "Section": "Portrait Configs",
      "AllowBlank": false
    },
    "Background Color": {
      "AllowValues": "Ancient, Brick, Cheesy, Cyan, Dino, Midnight, Parchment, PinkCake, Plum, Quartz, Rhubarb, SeaFoam, Shadow, Shimmer, Strawberry, Sugar, Urchin, Wild",
      "Default": "Cyan",
      "Section": "Portrait Configs",
      "AllowBlank": false
    },
    "Frame Color": {
      "AllowValues": "Baby_Pink, Baby_Purple, Bamboo, Birch, Black, Cold_Metal, Dark_Metal, Green, Mahogany, Oak, Vanilla, Walnut, Wizard_Furniture",
      "Default": "Vanilla",
      "Section": "Portrait Configs",
      "AllowBlank": false
    },
    "Is Special Portrait": {
      "AllowValues": "False, True",
      "Default": "False",
      "Section": "Portrait Configs",
      "AllowBlank": false
    },
    "What Special Portrait": {
      "AllowValues": "Dancer, NightSky, MagicSailor, Cupid",
      "Default": "NightSky",
      "Section": "Portrait Configs",
      "AllowBlank": false
    },
    "Emily Polyamoury Events": {
      "AllowValues": "True, False",
      "Default": "False",
      "Section": "Extra Content",
      "AllowBlank": false,
      "Description": "Adds new events if player's in a polycule with both Emily and Quinn. ONE event currently available."
    },
    "Shane Polyamoury Events": {
      "AllowValues": "True, False",
      "Default": "False",
      "Section": "Extra Content",
